class processes {
    static int body = 0;
    
	static void Run1()
	{
		body = 1;
		trace("Run1 -- 1");
		yield;
		trace("Run1 -- 2");
		scheduler.Remove(self);
	}
	
	static void Run2()
	{
		body = 2;
		trace("Run2 -- 1");
		yield;
		trace("Run2 -- 2");
		assert(body == 2);
		scheduler.Remove(self);
	}
};



class Main {

    activate static void Run()
    {
		async processes.Run2();
		async processes.Run1();
		scheduler.Remove(self);
    }
};
